{
    "name": "DoceboIntegration",

    "namespace": "Stanford\\DoceboIntegration",

    "description": "This module enrolls users requesting access to E-Reg into Docebo learning plans according to their role.",

    "documentation": "README.md",

    "framework-version": 16,

    "authors": [
        {
            "name": "Ihab Zeedia",
            "email": "ihab.zeedia@stanford.edu",
            "institution": "Stanford Health Care"
        }
    ],


    "enable-every-page-hooks-on-system-pages": false,
    "links": {
        "project": [
            {
                "name": "Docebo Test Page",
                "icon": "fas fa-check-circle",
                "url": "pages/test.php",
                "show-header-and-footer": true
            }
        ],
        "control-center": [

        ]
    },

    "no-auth-pages": [

    ],

    "project-settings": [
        {
            "key": "e-reg-request-forms",
            "name": "<b>E-Reg Request Survey</b><br/>The survey form that a user will be enrolled in Docebo learning plan after completion.",
            "required": true,
            "type": "form-list"
        }
    ],

    "system-settings": [
        {
            "key": "google-project-id",
            "name": "<b>Google Cloud Project ID</b><br/>The Google Cloud Project ID that contains the secrets used for authentication to Docebo API.",
            "required": true,
            "type": "text"
        },
        {
            "key": "docebo-access-token",
            "name": "<b>Docebo Access Token</b><br/>Access token for Docebo API.",
            "required": false,
            "type": "text"
        },
        {
            "key": "docebo-refresh-token",
            "name": "<b>Docebo Refresh Token</b><br/>Refresh token for Docebo API.",
            "required": false,
            "type": "text"
        },
        {
            "key": "docebo-token-expiry",
            "name": "<b>Docebo Access token Expiry timestamp</b><br/>Timestamp when the access token expires.",
            "required": false,
            "type": "text"
        }
    ],

    "crons": [

    ],

    "compatibility": {
        "php-version-min": "",
        "php-version-max": "",
        "redcap-version-min": "",
        "redcap-version-max": ""
    }
